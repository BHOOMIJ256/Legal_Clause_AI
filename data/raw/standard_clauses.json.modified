      "clause_id": "CLS010",
      "clause_number": 10,
      "title": "Subcontracting",
      "text": "No subcontracting without written consent.",
      "category": "Operations",
      "metadata": {
        "keywords": [
          "subcontracting",
          "consent"
        ],
        "importance": "medium",
